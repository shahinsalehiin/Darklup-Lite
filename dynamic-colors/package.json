{
  "name": "mahbub-parcel",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "watch": "parcel watch src/**/*.* --dist-dir ../assets/es-js/",
    "start": "npm run watch",
    "startIndex": "npx parcel src/index.js --dist-dir ../assets/es-js/",
    "startPresets": "npx parcel src/presets.js --dist-dir ../assets/es-js/",
    "production": "npx parcel build src/index.js",
    "buildIndex": "npx parcel build src/index.js --dist-dir ../assets/es-js/ && uglifyjs ../assets/es-js/index.js -o ../assets/es-js/index.js --mangle --mangle-props",
    "buildOnlyIndex": "npx parcel build src/index.js --dist-dir ../assets/es-js/",
    "uglifyOnlyIndex": "uglifyjs ../assets/es-js/index.js -o ../assets/es-js/index.js --mangle --mangle-props",
    "buildPresets": "npx parcel build src/presets.js --dist-dir ../assets/es-js/ && uglifyjs ../assets/es-js/presets.js -o ../assets/es-js/presets.js --mangle --mangle-props",
    "buildOnlyPresets": "npx parcel build src/presets.js --dist-dir ../assets/es-js/",
    "uglifyOnlyPresets": "uglifyjs ../assets/es-js/presets.js -o ../assets/es-js/presets.js --mangle --mangle-props",
    "buildb": "npx parcel build src/*.js --dist-dir ../assets/es-js/ && uglifyjs ../assets/es-js/*.js -o ../assets/es-js/index.js --mangle --mangle-props",
    "build": "npx parcel build src/index.js --dist-dir ../assets/es-js/",
    "uglify": "uglifyjs ../assets/es-js/index.js -o ../assets/es-js/index.js --mangle --mangle-props"
  },
  "author": "Mahbub",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "parcel": "^2.8.3",
    "terser": "^5.17.1",
    "uglify-js": "^3.17.4"
  },
  "dependencies": {
    "jquery": "^3.6.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "tinycolor2": "^1.6.0"
  }
}
