{
  "name": "mahbub-parcel",
  "version": "1.0.0",
  "description": "",
  "source": "src/index.js",
  "main": "../assets/js/index.js",
  "scripts": {
    "watch": "parcel watch",
    "start": "npx parcel src/index.js --dist-dir ../assets/js/",
    "production": "npx parcel build src/index.js",
    "buildx": "npx parcel build src/index.js --dist-dir ../assets/js/ && uglifyjs ../assets/js/index.js -o ../assets/js/index.js --mangle --mangle-props",
    "build2": "npx parcel build src/index.js --dist-dir ../assets/js/ && uglifyjs ../assets/js/index.js -o ../assets/js/index.js --mangle --mangle-props --config-file uglify.config.json",
    "buildt": "npx parcel build src/index.js --dist-dir ../assets/js/ && npx terser ../assets/js/index.js -o ../assets/js/index.js --mangle --mangle-props",
    "build": "npx parcel build src/index.js --dist-dir ../assets/js/",
    "uglify": "uglifyjs ../assets/js/index.js -o ../assets/js/index.js --mangle --mangle-props",
    "uglify2": "uglifyjs ../assets/js/index.js -o ../assets/js/index.js --mangle --mangle-props --config-file uglify.config.json"
  },
  "author": "Mahbub",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "parcel": "^2.8.3",
    "terser": "^5.17.1",
    "uglify-js": "^3.17.4"
  },
  "dependencies": {
    "jquery": "^3.6.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "tinycolor2": "^1.6.0"
  }
}
